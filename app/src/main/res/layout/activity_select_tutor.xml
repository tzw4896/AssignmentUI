<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.assignmentui.SelectTutor">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="202dp"
        android:layout_height="19dp"
        android:text="ilikeweed@staff.tarc.edu.my"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintLeft_toRightOf="@+id/imageButton3"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="202dp"
        android:layout_height="19dp"
        android:text="khookimxiang@staff.tarc.edu.my"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintLeft_toRightOf="@+id/imageButton4"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="134dp"
        android:layout_height="21dp"
        android:text="Mr. Khoo Kim Xiang"
