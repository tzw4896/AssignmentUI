    <TextView
        android:id="@+id/textView6"
        android:layout_width="195dp"
        android:layout_height="21dp"
        android:text="laikietxiang@staff.tarc.edu.my"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintLeft_toRightOf="@+id/imageButton"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="134dp"
        android:layout_height="21dp"
        android:text="Mr. Thiang Low Weed"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:layout_constraintLeft_toRightOf="@+id/imageButton3"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="134dp"
        android:layout_height="21dp"
        android:text="Mr. Lee Wai Thien"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintLeft_toRightOf="@+id/imageButton2"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="134dp"
        android:layout_height="21dp"
        android:text="Dr. Lai Kiet Xiang"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/imageButton"
